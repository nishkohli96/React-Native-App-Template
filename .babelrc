{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": ["./src"],
                "extensions": [".js", ".ios.js", ".android.js"],
                "alias": {
                    "@App":"./app",
                    "@Fonts": "./app/assets/fonts",
                    "@Images": "./app/assets/images",
                    "@Themes": "./app/assets/themes",
                    "@Components": "./app/components",
                    "@StyledComps": "./app/components/styled-components",
                    "@Context": "./app/context",
                    "@I18Next": "./app/i18n",
                    "@Screens": "./app/screens",
                    "@Utils": "./app/utils"
                }
            }
        ]
    ]
}